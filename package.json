{
  "name": "obsidian-filename-heading-sync",
  "version": "1.0.0",
  "description": "Obsidian plugin for keeping the filename with the first heading of a file in sync",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "rollup --config rollup.config.ts --bundleConfigAsCjs -w",
    "build": "rollup --config rollup.config.ts --bundleConfigAsCjs",
    "format": "npx prettier --config .prettierrc --write *.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "jest": "^29.7.0",
    "obsidian": "^1.7.2",
    "obsidian-typings": "^3.12.0",
    "prettier": "^3.4.2",
    "rollup": "^4.29.2",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
